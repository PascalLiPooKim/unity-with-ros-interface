<?xml version="1.0"?>

<launch>
<include file="$(find custom_husky_gazebo)/launch/husky_gazebo_course_3_hard.launch" />

<include file="$(find custom_husky_gazebo)/launch/husky_teleop_v5_5FAS.launch" />


<include file="$(find commands_to_unity)/launch/course_3_hard_kbhit.launch" />

<arg name="record_data" default="false"/>
<arg name="test_case" default="V"/>
<arg name="candidate_num" default="1"/>

<group if="$(arg record_data)">
<node pkg="rosbag" type="record" name="husky_course_3_hard_5FAS"
       args="record -o $(find custom_husky_gazebo)/bags/husky_course_3_hard_5FAS_$(arg test_case)_$(arg candidate_num) /ground_truth/state" />
</group>

<node pkg="commands_to_unity" name="get_model_state" 
    type="get_model_state.py" output="screen"/>

</launch>
